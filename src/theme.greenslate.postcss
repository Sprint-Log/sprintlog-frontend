:root {
  /* =~= Theme Properties =~= */
  --theme-font-color-base: var(--color-primary-200);
  --theme-font-color-dark: var(--color-primary-300);
  --theme-rounded-base: 9999px;
  --theme-rounded-container: 8px;
  --theme-border-base: 1px;
  /* =~= Theme On-X Colors =~= */
  --on-primary: 0 0 0;
  --on-secondary: 0 0 0;
  --on-tertiary: 0 0 0;
  --on-success: 0 0 0;
  --on-warning: 0 0 0;
  --on-error: 0 0 0;
  --on-surface: 255 255 255;
  /* =~= Theme Colors  =~= */
  /* primary | #7da9cb */
  --color-primary-50: 236 242 247; /* ⬅ #ecf2f7 */
  --color-primary-100: 229 238 245; /* ⬅ #e5eef5 */
  --color-primary-200: 223 234 242; /* ⬅ #dfeaf2 */
  --color-primary-300: 203 221 234; /* ⬅ #cbddea */
  --color-primary-400: 164 195 219; /* ⬅ #a4c3db */
  --color-primary-500: 125 169 203; /* ⬅ #7da9cb */
  --color-primary-600: 113 152 183; /* ⬅ #7198b7 */
  --color-primary-700: 94 127 152; /* ⬅ #5e7f98 */
  --color-primary-800: 75 101 122; /* ⬅ #4b657a */
  --color-primary-900: 61 83 99; /* ⬅ #3d5363 */
  /* secondary | #5be157 */
  --color-secondary-50: 230 251 230; /* ⬅ #e6fbe6 */
  --color-secondary-100: 222 249 221; /* ⬅ #def9dd */
  --color-secondary-200: 214 248 213; /* ⬅ #d6f8d5 */
  --color-secondary-300: 189 243 188; /* ⬅ #bdf3bc */
  --color-secondary-400: 140 234 137; /* ⬅ #8cea89 */
  --color-secondary-500: 91 225 87; /* ⬅ #5be157 */
  --color-secondary-600: 82 203 78; /* ⬅ #52cb4e */
  --color-secondary-700: 68 169 65; /* ⬅ #44a941 */
  --color-secondary-800: 55 135 52; /* ⬅ #378734 */
  --color-secondary-900: 45 110 43; /* ⬅ #2d6e2b */
  /* tertiary | #c2e2a9 */
  --color-tertiary-50: 246 251 242; /* ⬅ #f6fbf2 */
  --color-tertiary-100: 243 249 238; /* ⬅ #f3f9ee */
  --color-tertiary-200: 240 248 234; /* ⬅ #f0f8ea */
  --color-tertiary-300: 231 243 221; /* ⬅ #e7f3dd */
  --color-tertiary-400: 212 235 195; /* ⬅ #d4ebc3 */
  --color-tertiary-500: 194 226 169; /* ⬅ #c2e2a9 */
  --color-tertiary-600: 175 203 152; /* ⬅ #afcb98 */
