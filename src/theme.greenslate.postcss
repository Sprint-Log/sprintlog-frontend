  --color-surface-800: 20 44 57; /* ⬅ #142c39 */
  --color-surface-900: 17 36 47; /* ⬅ #11242f */
}
body {
  /* background-color:hsla(0,0%,0%,1); */
  background-image: radial-gradient(at 38% 63%, hsla(59, 10%, 10%, 1) 0px, transparent 50%),
    radial-gradient(at 100% 22%, hsla(324, 50%, 10%, 1) 0px, transparent 50%),
    radial-gradient(at 53% 36%, hsla(336, 30%, 10%, 1) 0px, transparent 50%);
}
