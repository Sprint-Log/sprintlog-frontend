  --color-tertiary-900: 95 111 83; /* ⬅ #5f6f53 */
  /* success | #e1608c */
  --color-success-50: 251 231 238; /* ⬅ #fbe7ee */
  --color-success-100: 249 223 232; /* ⬅ #f9dfe8 */
  --color-success-200: 248 215 226; /* ⬅ #f8d7e2 */
  --color-success-300: 243 191 209; /* ⬅ #f3bfd1 */
  --color-success-400: 234 144 175; /* ⬅ #ea90af */
  --color-success-500: 225 96 140; /* ⬅ #e1608c */
  --color-success-600: 203 86 126; /* ⬅ #cb567e */
  --color-success-700: 169 72 105; /* ⬅ #a94869 */
  --color-success-800: 135 58 84; /* ⬅ #873a54 */
